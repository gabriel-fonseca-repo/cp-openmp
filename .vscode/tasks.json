{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "Buildar arquivo",
			"command": "/usr/bin/gcc",
			"args": ["-fdiagnostics-color=always", "-W", "-fopenmp", "-g", "${file}", "-o", "${workspaceRoot}/out/${fileBasenameNoExtension}", "-lm"],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": ["$gcc"],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		}
	],
	"version": "2.0.0"
}
